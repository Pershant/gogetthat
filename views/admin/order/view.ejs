<%- include('../partials/header'); %>


  <!-- Main Content -->
  <div class="main-content">
    <section class="section">
      <div class="section-header">
        <h1 id="animated_box">
          <%= currentModule; %>
        </h1>
      </div>

      <div class="section-body animate__animated  animate__backInUp">

        <div class="row">
          <div class="col-8 offset-2">
            <div class="card">
              <div class="card-header">
                <h4>
                  <%= currentSubModule %>
                </h4>
              </div>
              <div class="card-body">
                <!-- <form class="form_advanced_validation" method="POST" autocomplete="off" enctype="multipart/form-data"> -->

                <div class="row">
                  <div class="col-6">
                    <div class="form-group">
                      <label>Order ID</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-fingerprint"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name" value="<%= order.orderNo; %>" required
                          disabled>
                      </div>
                    </div>
                  </div>

                  <div class="col-6">
                    <div class="form-group">
                      <label>Order Status</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-check"></i>
                          </div>
                        </div>
                        <!-- <input type="text" class="form-control" name="name" value="<%= order.orderStatusText; %>" required
                          disabled> -->
                        <select class="form-control changeOrderRequestStatus" model="<%=model%>"
                          model_title="<%=modelTitle%>" model_id="<%=order.id%>" field="orderStatus"
                          style="background-color: <%=orderStatus[order.orderStatus].backgroundCssColor;%>; color: #fff; font-weight: 600;"
                          disabled>
                          <% for (let status in orderStatus) { %>
                            <option value="<%=status%>" <%=status==order.orderStatus ? 'selected' : '' ; %>
                              <%= status < order.orderStatus ? 'disabled' : '' ; %>
                                style="background: <%= orderStatus[status].backgroundCssColor %>; color: #fff;
                                  font-weight: 600;">
                                  <%=orderStatus[status].value%>
                            </option>
                            <% } %>
                        </select>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="row">
                  <div class="col-6">
                    <div class="form-group">
                      <label>Customer</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-user"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name"
                          value="<%= order.customer.userDetail.name; %>" required disabled>
                      </div>
                    </div>
                  </div>

                  <!-- <div class="col-6">
                    <div class="form-group">
                      <label>Seller</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-user"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name"
                          value="<%= order.vendor.vendorDetail.name; %>" required disabled>
                      </div>
                    </div>
                  </div> -->
                </div>

                <div class="row">
                  <div class="col-6">
                    <div class="form-group">
                      <label>QTY</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-balance-scale"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name" value="<%= order.qty; %>" required disabled>
                      </div>
                    </div>
                  </div>

                  <div class="col-6">
                    <div class="form-group">
                      <label>Net Amount</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-dollar-sign"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name" value="<%= order.netAmount; %>" required
                          disabled>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="row">
                  <div class="col-6">
                    <div class="form-group">
                      <label>Order Tracking Id</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-dollar-sign"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="order_tracking_id"
                          value="<%= order.order_tracking_id; %>" required disabled>
                      </div>
                    </div>
                  </div>

                  <div class="col-6">
                    <div class="form-group">
                      <label>Shipping Charges</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-dollar-sign"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name" value="<%= order.shippingCharges; %>"
                          required disabled>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="row">
                  <div class="col-6">
                    <div class="form-group">
                      <label>Order Tracking Url</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-dollar-sign"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="order_tracking_url"
                          value="<%= order.order_tracking_url; %>" required disabled>
                      </div>
                    </div>
                  </div>

                  <div class="col-6">
                    <div class="form-group">
                      <label>Total</label>
                      <div class="input-group">
                        <div class="input-group-prepend">
                          <div class="input-group-text">
                            <i class="fas fa-dollar-sign"></i>
                          </div>
                        </div>
                        <input type="text" class="form-control" name="name" value="<%= order.netAmount; %>" required
                          disabled>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- <button productId="<%=order.productId%>" class="btn btn-outline-success viewProduct" >View Product Detail</button> -->

                <div class="form-group row mb-4">
                  <div class="col-sm-12 col-md-7 offset-4">
                    <input type="hidden" class="form-control" name="id" value="<%= order.id %>">
                    <button type="button" class="btn btn-secondary cancel_btn">Back</button>
                    <!-- <button type="submit" class="btn btn-primary">Update</button> -->
                  </div>
                </div>
                <!-- </form> -->
              </div>
            </div>
          </div>
        </div>
      </div>

    </section>
  </div>


  <%- include('../partials/footer'); %>

    <script>
      $(document).ready(function () {

        $(".viewProduct").click(function () {

          var productId = $(this).attr("productId");
          //window.location=`/admin/product/view/${productId}`
          //alert(productId,"The paragraph was clicked.");
          window.open(`/admin/product/view/${productId}`);
        });

      });
    </script>