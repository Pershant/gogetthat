                <section class="store_adrs">
                  <div class="container-fluid">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="store_adrs2">
                          <h2>Billing Addresses <a href data-toggle="modal" data-target="#add_billing"
                              class="log_in_btn"> Add Billing
                              Address</a></h2>
                        </div>
                        <div class="row">
                          <% find_address.forEach((address)=>{
                            if(address.type==1) {
                            %>

                            <div class="col-md-6">
                              <div class="store_adrs3">
                                <h2>
                                  <%=address.name%>
                                    <%=address.last_name%>
                                      <div class="custom-control custom-radio">
                                        <input type="radio" id="customRadio1" name="customRadio"
                                          class="custom-control-input" checked="">
                                        <label class="custom-control-label" for="customRadio1">Primary</label>
                                      </div>
                                </h2>
                                <h4>
                                  <%=session.user.phone%>
                                </h4>
                                <p>
                                  <%=address.address_line_1%>
                                    <%=address.address_line_2%>
                                      <%=address.city%>
                                        <%=address.state%>
                                          <%=address.country%>
                                </p>
                                <p>
                                  <%=address.zip_code%> <span>
                                      <a href data-toggle="modal" data-target="#add_billing"> <img src="website/images/edit2.png"> </a>
                                      <a class="del_address" address_id="<%=address.id%>" style="cursor: pointer;"> <img
                                          src="website/images/edit.png"> </a>
                                    </span>
                                </p>
                              </div>
                            </div>
                            <% } })%>
                        </div>
                      </div>

                      <div class="col-md-6">
                        <div class="store_adrs2 right_Space">
                          <h2>Shipping Addresses <a href="" data-toggle="modal" data-target="#add_billing2"
